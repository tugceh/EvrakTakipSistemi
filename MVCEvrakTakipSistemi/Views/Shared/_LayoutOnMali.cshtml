
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_LayoutOnMali</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div class="container">
        <div class="jumbotron">
            @Html.ActionLink("Index", "Index", "OnMali", new { @class = "col-2 btn btn-primary" })
            @Html.ActionLink("Bekleyen Evraklar", "Bekleyen", "OnMali", new { @class = "col-2 btn btn-primary" })
            @Html.ActionLink("İncelenen Evraklar", "Incelenen", "OnMali", new { @class = "col-2 btn btn-primary" })
            @Html.ActionLink("Hatalı Evraklar", "Hata", "OnMali", new { @class = "col-2 btn btn-primary" })
            @Html.ActionLink("Başarılı Evraklar", "Basarili", "OnMali", new { @class = "col-2 btn btn-primary" })
        </div>
    </div>

    <main role="main" class="container">
        @RenderBody()
    </main>
</body>
</html>

